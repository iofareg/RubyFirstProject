<% @posts.each do |post| %>
<div class="card">
    <div class="card-header">
        <%= post.title%>
    </div>
    <div class="card-body">
        <p class="card-text"><%= post.body%></p>
        <%= link_to 'Show', post_path(post), :class => 'btn btn-primary'%>
    </div>
</div>  
<br>
<% end %>


